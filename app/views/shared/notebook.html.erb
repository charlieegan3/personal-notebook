<%= render 'shared/account' %>

<h1>Notebook</h1>
<%= link_to 'Multi Edit', multi_edit_path %>

<h2>Sections</h2>
<%= render 'sections/form', section: Section.new %>
<%= render 'sections/list', sections: @sections %>

<h2>Recent Notes</h2>
<ul>
  <% @notes.each do |note| %>
    <li><%= link_to note.title, note, class: :data %></li>
  <% end %>
</ul>
